 ⎕TRAP←(999 'C' '. ⍝ Deliberate error')(0 'N')

 :If batchFlag
     R←∆NoBatchTest
 :Else

     R←∆Failed
     :Trap 11
         {}'MoreTests/'##.Maker.RunInno'inno'
         →FailsIf 1
     :Else
         dmx←⎕DMX
         →PassesIf'Inno compiler did abort'≡⊃dmx.DM
     :EndTrap

     R←∆OK

 :EndIf
